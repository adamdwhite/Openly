<p id="notice"><%= notice %></p>

<h1>Counselors</h1>

<table>
  <thead>
    <tr>
      <th>First Name</th>
      <br>
      <th>Last Name</th>
      <br>
      <th>Age</th>
      <br>
      <th>Gender</th>
      <br>
      <th>Zipcode</th>
      <br>
      <th>Image</th>
      <br>
      <th>Social Security Number</th>
      <br>
      <th>Occupational Title</th>
      <br>
      <th>U.S. State of License</th>
      <br>
      <th>License Number</th>
      <br>
      <th>Payment</th>
      <br>
      <th>Agreement</th>
      <br>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @counselors.each do |counselor| %>
      <tr>
        <td><%= counselor.first_name %></td>
        <td><%= counselor.last_name %></td>
        <td><%= counselor.age %></td>
        <td><%= counselor.gender %></td>
        <td><%= counselor.zipcode %></td>
        <td><%= counselor.image %></td>
        <td><%= counselor.ssn %></td>
        <td><%= counselor.occ_title %></td>
        <td><%= counselor.occ_state %></td>
        <td><%= counselor.license %></td>
        <td><%= counselor.payment %></td>
        <td><%= counselor.agreement %></td>
        <td><%= link_to 'Show', counselor, id: "showbtn" %></td>
        <td><%= link_to 'Edit', edit_counselor_path(counselor), id: "editbtn" %></td>
        <td><%= link_to 'Destroy', counselor, method: :delete, data: { confirm: 'Are you sure?' }, id: "deletebtn" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Submit a New Counselor Application', new_counselor_path, id: "signup" %>
